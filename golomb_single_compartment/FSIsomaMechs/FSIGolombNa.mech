vna = [50]
gna = [112.5]
thetam = [-24]
sigmam = [11.5]
thetah = [-58.3]
sigmah = [-6.7]
tauminh = [0.5]
taumaxh = [14]
sigmath = [-12]
phih = [-60]
 
minf(X) = 1./(1+exp(-(X-thetam)./sigmam))
hinf(X) = 1./(1+exp(-(X-thetah)./sigmah)) 
tauh(X) = tauminh + taumaxh./(1+exp(-(X-phih)./sigmath)) 
FSIGolombNa(X,h) = gna.*h.*(minf(X).^3).*(X-vna)

h' = (hinf(X)-h)./tauh(X);
h(0) = 0.05
 
@current += -FSIGolombNa(X,h)
 
